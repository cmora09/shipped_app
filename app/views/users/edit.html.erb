<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<%= form_for (@user) do |f| %>
	<%= f.label :First_name %>
	<%= f.text_field :fname %>
	<%= f.label :Last_name %>
	<%= f.text_field :lname %>
	<%= f.label :email %>
	<%= f.text_field :email %>
	<%= f.label :Username %>
	<%= f.text_field :username %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit %>
<% end %>
<br>
<%= link_to "Delete Account", user_path(@user), method: :delete %>