<% if @user.follows.find_by(boat_id: all.id) %>
		<%= form_for(@user.follows.find_by(boat_id: all.id), html: {method: :delete}) do |f| %>
			<%= f.submit 'Unfollow' %>
		<% end %>
	<% else %>
		<%= form_for(@user.follows.build) do |f| %>
				<%= hidden_field_tag :boat_id, all.id %>
				<%= f.submit "Follow" %>
	<% end %>
<% end %>