<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for(@user) do |f| %>
	<%= f.label :First_Name %>
	<%= f.text_field :fname %>
	<%= f.label :Last_Name %>
	<%= f.text_field :lname %>
	<%= f.label :email %>
	<%= f.text_field :email %>
	<%= f.label :Username %>
	<%= f.text_field :username %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit %>
<% end %>
