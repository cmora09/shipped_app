<h1>Welcome to Zorg Logistics</h1>

<h2> Log-in, Captain. </h2>

<%= form_tag login_path do %>
	<%= label_tag(:username) %>
	<%= text_field_tag(:username) %>
	<%= label_tag(:password) %>
	<%= password_field_tag(:password) %>
	<%= submit_tag "Log-in" %>
<% end %>

<h1>Not a member? Sign up!</h1>
<a href="/users/new">Sign Up</a>